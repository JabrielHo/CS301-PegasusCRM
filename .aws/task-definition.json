{
    "family": "client-ecs-task",
    "containerDefinitions": [
        {
            "name": "client-ecs-container",
            "image": "163683790602.dkr.ecr.ap-southeast-1.amazonaws.com/client_service:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "client-ecs-container-5001-tcp",
                    "containerPort": 5001,
                    "hostPort": 5001,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "secrets": [
                {
                    "name": "AWS_REGION",
                    "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:163683790602:secret:githubactions-ErtwVS:AWS_REGION"
                },
                {
                    "name": "AWS_ACCESS_KEY_ID",
                    "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:163683790602:secret:githubactions-ErtwVS:AWS_ACCESS_KEY_ID"
                },
                {
                    "name": "DB_USER",
                    "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:163683790602:secret:githubactions-ErtwVS:DB_USER"
                },
                {
                    "name": "AWS_SECRET_ACCESS_KEY",
                    "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:163683790602:secret:githubactions-ErtwVS:AWS_SECRET_ACCESS_KEY"
                },
                {
                    "name": "DB_NAME",
                    "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:163683790602:secret:githubactions-ErtwVS:DB_NAME"
                },
                {
                    "name": "DB_HOST",
                    "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:163683790602:secret:githubactions-ErtwVS:DB_HOST"
                },
                {
                    "name": "SES_SENDER_EMAIL",
                    "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:163683790602:secret:githubactions-ErtwVS:SES_SENDER_EMAIL"
                },
                {
                    "name": "BUCKET_NAME",
                    "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:163683790602:secret:githubactions-ErtwVS:BUCKET_NAME"
                },
                {
                    "name": "DB_PASSWORD",
                    "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:163683790602:secret:githubactions-ErtwVS:DB_PASSWORD"
                },
                {
                    "name": "PORT",
                    "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:163683790602:secret:githubactions-ErtwVS:PORT"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/client-ecs-task",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::163683790602:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::163683790602:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
}
}