{
    "family": "account-ecs-task",
    "containerDefinitions": [
        {
            "name": "account-ecs-container",
            "image": "163683790602.dkr.ecr.ap-southeast-1.amazonaws.com/account_service:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "account-ecs-container-5003-tcp",
                    "containerPort": 5003,
                    "hostPort": 5003,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "SECRET_STRING",
                    "value": "arn:aws:secretsmanager:ap-southeast-1:163683790602:secret:githubactionsAccount-BtJwVT"                }
                ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/account-ecs-task",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::163683790602:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::163683790602:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}